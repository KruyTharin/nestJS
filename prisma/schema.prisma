generator client {
  provider        = "prisma-client-js"
  output   = "../src/prisma/generated"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String   @id @default(uuid())
  username String   @unique
  email    String   @unique
  role     Role     @default(USER)
  gender String?
  password String

  @@map("user")
}

enum Role {
  USER
  ADMIN
}

